{
  "version": 2,
  "name": "syrian-martyrs-bot",
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook"
    },
    {
      "src": "/",
      "dest": "/api/webhook"
    },
    {
      "src": "/health",
      "dest": "/api/webhook"
    }
  ],
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "BOT_TOKEN": "BOT_TOKEN",
    "ADMIN_USER_ID": "ADMIN_USER_ID",
    "MONGODB_URI": "MONGODB_URI",
    "NODE_ENV": "production"
  },
  "build": {
    "outputDirectory": ""
  }
}